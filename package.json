{
  "name": "fernfolio-11ty-template",
  "version": "1.0.0",
  "description": "The super simple portfolio template built with Eleventy and Netlify CMS",
  "scripts": {
    "start": "npx concurrently \"postcss ./src/_includes/assets/tailwind.css -d ./src/_includes/assets/css -w\" \"npx @11ty/eleventy --serve\"",
    "build": "npm-run-all build:css build:html",
    "build:css": "npx postcss ./src/_includes/assets/tailwind.css -d ./src/_includes/assets/css",
    "build:html": "npx @11ty/eleventy --input=./src --output=./_site"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tylerMRoderick/fernfolio-11ty-template"
  },
  "author": {
    "name": "Tyler M. Roderick",
    "email": "troderick@protonmail.com",
    "url": "https://www.tylerroderick.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tylerMRoderick/fernfolio-11ty-template/issues"
  },
  "homepage": "https://github.com/tylerMRoderick/fernfolio-11ty-template",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cssnano": "^5.1.4",
    "luxon": "^1.25.0",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.3.0",
    "postcss": "^8.4.8",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "tailwindcss": "^3.0.23"
  },
  "dependencies": {
    "@11ty/eleventy-navigation": "^0.1.6",
    "clean-css": "^4.2.1",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "uglify-es": "^3.3.9"
  }
}
